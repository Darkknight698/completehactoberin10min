#include <stdio.h>
int main()
{
   char str_check[1000], ch;
   int i, frequency = 0;
   printf("Enter a string: ");
   gets(str_check);
   printf("Enter a character to find the frequency: ");
   scanf("%c",&ch);
   for(i = 0; str_check[i] != '\0'; ++i)
   {    
       if(ch == str_check[i])
           frequency++;
   }
   printf("Frequency of %c = %d", ch, frequency);
   return 0;
}
