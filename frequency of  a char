#include <stdio.h>
int main()
{
   char a[300], ch;
   int m, f = 0;
   printf("Enter a string: ");
   gets(a);
   printf("Enter a character to find the frequency: ");
   scanf("%c",&ch);
   for(m = 0; a[m] != '\0'; ++m)
   {    
       if(ch == a[m])
           f++;
   }
   printf("Frequency of %c = %d", ch, f);
   return 0;
}
