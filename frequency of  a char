#include <stdio.h>
int main()
{
   char s[1000], ch;
   int i, f = 0;
   printf("Enter a user string : ");
   gets(s);
   printf("Enter a character you want to  find the frequency: ");
   scanf("%c",&ch);
   for(i = 0; s[i] != '\0'; ++i)
   {    
       if(ch == s[i])
           f++;
   }
   printf("Frequency of %c = %d", ch, f);
   return 0;
}
