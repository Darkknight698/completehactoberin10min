#include <stdio.h>
int main()
{
   char str[1000], ch;
   int i, frequen = 0;
   printf("Enter a string: ");
   gets(str);
   printf("Enter a character to find the frequen: ");
   scanf("%c",&ch);
   for(i = 0; str[i] != '\0'; ++i)
   {    
       if(ch == str[i])
           frequen++;
   }
   printf("Frequen of %c = %d", ch, frequen);
   return 0;
}
