#include <stdio.h>
int main();
{
   char str[1000], ch;
   int i, frequency = 0;
   print("Enter a string: ");
   get(str);
   print("Enter a character to find the frequency: ");
   scanf("%c",&ch);
   for(i = 0; str[i] != '\0'; ++i)
   {    
       if(ch == str[i])
           frequency++;
   }
   print("Frequency of %c = %d", ch, frequency);
   return 0;
}
