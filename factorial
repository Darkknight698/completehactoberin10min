//ALGORITHM

step 1. Start
step 2. Read the number n
step 3. [Initialize]
        i=1, fact=1
step 4. Repeat step 4 through 6 until i=n
step 5. fact=fact*i
step 6. i=i+1
step 7. Print fact
step 8. Stop

//PROGRAM

#include<stdio.h>
#include<conio.h>
void main()
{
    int n,i=1,f=1;
    clrscr();
     
    printf("\n Enter The Number:");
    scanf("%d",&n);
     
    //LOOP TO CALCULATE FACTORIAL OF A NUMBER
    while(i<=n)
    {
        f=f*i;
        i++;
    }
     
    printf("\n The Factorial of %d is %d",n,f);
    getch();
}
