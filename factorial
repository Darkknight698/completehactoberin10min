//ALGORITHM

 Start
 Read the number n
 [Initialize]
  i=1, fact=1
  Repeat step 4 through 6 until i=n
  fact=fact*i
  i=i+1
 Print fact
 Stop

//PROGRAM

#include<stdio.h>
#include<conio.h>
int main()
{
 int n,x,fact=1;
 printf("Enter any number : ");
 scanf("%d", &n);
 for(x=1; x<=n; x++)
    fact = fact * x;
 printf("Factorial value of %d = %d",n,fact);
 return 0;
}
