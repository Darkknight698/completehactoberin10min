//ALGORITHM

step 1. Start
step 2. Read the number n
step 3. [Initialize]
        i=1, fact=1
step 4. Repeat step 4 through 6 until i=n
step 5. fact=fact*i
step 6. i=i+1
step 7. Print fact
step 8. Stop

//PROGRAM
#include<conio.h>
#include<stdio.h>
void main()
{
	int n,i;
	long int f=1;
	
	printf("enter the number tobe entered");
	scanf("%d",&n);
	
	for(i=1;i<=n;i++)
	{
	f=f*i;
    }
    printf("factorial=%d",f);
    getch();
}
