#include<stdio.h>
#include<stdlib.h> 
int main()
{
   int n;
   system("color fd");
   printf("Enter an integer\n");
   scanf("%d",&n);
 
   if ( n & 1 == 1 )
   {
      system("color fd");
      printf("Odd\n");
   }
   else
   {
      system("color fd");
      printf("Even\n");
    }
 
   return 0;
}
