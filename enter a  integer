/*THIS APPLICATION TAKES ANY INTIGER AS INPUT AND DISPLAYS THE INTIGER AS OUTPUT */
#include <stdio.h>
int main()
{
    int number;
  /*ENTER THE INTIGER TO BE DISPLAYED HERE*/ 
    printf("Enter an integer:");
    
   
    scanf("%d",&number);  
    
  
    printf("You entered: %d", number);
    return 0;
}
