// this program copies srting from one array to another character by character
//time complexity of this program is : O(n)
#include <stdio.h>
int main()
{
    char s1[100], s2[100], i;
    print("Enter string s1: ")
    scan("%s",s1);
    for(i = 0; s1[i] != '\0'; ++i)
    {
        s2[i] = s1[i];
    }
    s2[i] = '\0';
    printf("String s2: %s", s2);
   
}
